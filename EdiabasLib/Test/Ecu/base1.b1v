;Base program for command execution test

TBEG "TEST_TAB1"
HEAD "VALUE", "NAME"
LINE "0x10", "LINE1"
LINE "0x20", "LINE2"
LINE "0x30", "LINE3"
LINE "0x40", "LINE4"
LINE "0x50", "LINE5"
TEND

TBEG "TEST_TAB2"
HEAD "VALUE", "NAME"
LINE "0x11", "LINE1"
LINE "0x21", "LINE2"
LINE "0x31", "LINE3"
LINE "0x41", "LINE4"
LINE "0x51", "LINE5"
LINE "0x61", "LINE6"
TEND

INITIALISIERUNG#
;    JOBNAME:INITIALISIERUNG
;    JOBCOMMENT:Initialisierung und Kommunikationsparameter
;    RESULT:DONE
;    RESULTTYPE:int
;    RESULTCOMMENT:1, wenn Okay
;
					shmset		"ID","BASE1_INIT"
					ergi		"DONE",#$1.I
					eoj
					eoj

ENDE#
;    JOBNAME:ENDE
;    JOBCOMMENT:Schliessen des SGBD
;    RESULT:DONE
;    RESULTTYPE:int
;    RESULTCOMMENT:1, wenn Okay
;
					shmset		"ID","BASE1_EXIT"
					eoj
					eoj

TEST_SHMID#
;    JOBNAME:TEST_SHMID
;    JOBCOMMENT:Test shm
;    RESULT:dynamic
;    RESULTTYPE:dynamic
;    RESULTCOMMENT:Test results
;
					shmget		S0,"ID"
					ergs		"SHMID1", S0
					eoj
					eoj

TEST_BASE1_TABLE1#
;    JOBNAME:TEST_BASE1_TABLE1
;    JOBCOMMENT:Test table
;    RESULT:dynamic
;    RESULTTYPE:dynamic
;    RESULTCOMMENT:Test results
;
					shmset		"ID","BASE1_TABLE1"

					tabset		"TEST_TAB1"
					clear		S2
					move		S2,"LINE3"
					tabseek		"NAME",S2
					clear		S0
					move 		S0,S1
					tabget		S0,"VALUE"
					ergs		"TABGET3",S0
					tabrows		L0
					ergl		"TABROWS",L0
					tabcols		L0
					ergl		"TABCOLS",L0
					
					enewset
					tabset		"TEST_TAB2"
					clear		S2
					move		S2,"LINE4"
					tabseek		"NAME",S2
					clear		S0
					move 		S0,S1
					tabget		S0,"VALUE"
					ergs		"TABGET4",S0
					tabrows		L0
					ergl		"TABROWS",L0
					tabcols		L0
					ergl		"TABCOLS",L0
					
					eoj
					eoj

